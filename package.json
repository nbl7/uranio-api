{
	"name": "uranio-api",
	"version": "0.0.1",
	"description": "Uranio API",
	"main": "dist/index.js",
	"exports": {
		".": "./dist/index.js",
		"./client": "./dist/client.js",
		"./types": "./dist/types.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"types": [
				"dist/types.d.ts"
			]
		}
	},
	"repository": "git@github.com:nbl7/uranio-api.git",
	"author": "Andrea Reni <mail@nbl7.net>",
	"scripts": {
		"build": "npx tsc -b",
		"dev": "npx tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/dev.js\"",
		"dev:generate:compile": "npx esbuild ./src/generate.ts --outfile=./generate/_generate_bundled.js --bundle --platform=node --watch",
		"dev:generate:run": "npx nodemon --watch ./generate/_generate_bundled.js -e ts ./generate/_generate_bundled.js urn_output_dir=./generate",
		"schema:reset": "yarn upgrade uranio-schema && npx npm-dts generate -L debug -r node_modules/uranio-schema -o ../../.uranio/generate/base/schema.d.ts",
		"dev:generate": "yarn schema:reset && npx concurrently \"yarn dev:generate:compile\" \"yarn dev:generate:run\""
	},
	"devDependencies": {
		"@types/cors": "^2.8.9",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.11",
		"@types/express-fileupload": "^1.2.2",
		"@types/jsonwebtoken": "^8.5.8",
		"concurrently": "^7.0.0",
		"esbuild": "^0.14.22",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.1",
		"express-fileupload": "^1.3.1",
		"jsonwebtoken": "^8.5.1",
		"lambda-multipart-parser": "^1.0.1",
		"uranio-schema": "ssh://git@github.com/nbl7/uranio-schema.git",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#new"
	}
}
