{
	"name": "uranio-api",
	"version": "0.0.1",
	"description": "Uranio API",
	"main": "dist/index.js",
	"bin": {
		"uranio-api-generate-schema": "dist/server/generate.js",
		"uranio-api": "dist/bin.js"
	},
	"exports": {
		".": "./dist/server.js",
		"./types": "./dist/server/types.js",
		"./client": "./dist/client.js",
		"./client/types": "./dist/client/types.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/server.d.ts"
			],
			"types": [
				"dist/server/types.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"client/types": [
				"dist/client/types.d.ts"
			]
		}
	},
	"repository": "git@github.com:nbl7/uranio-api.git",
	"author": "Andrea Reni <mail@nbl7.net>",
	"scripts": {
		"build": "npx tsc -b",
		"dev": "npx tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/dev.js\"",
		"generate:compile": "npx esbuild ./src/server/generate.ts --outfile=./generate/_generate_bundled.js --bundle --platform=node",
		"generate:schema": "yarn generate:compile && node ./generate/_generate_bundled.js urn_command=schema"
	},
	"devDependencies": {
		"@types/caller": "^1.0.0",
		"@types/cors": "^2.8.9",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.11",
		"@types/express-fileupload": "^1.2.2",
		"@types/jsonwebtoken": "^8.5.8",
		"esbuild": "^0.14.22",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git#master"
	},
	"dependencies": {
		"caller": "^1.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.1",
		"express-fileupload": "^1.3.1",
		"jsonwebtoken": "^8.5.1",
		"lambda-multipart-parser": "^1.0.1",
		"uranio-core": "git+ssh://git@github.com/nbl7/uranio-core.git#master",
		"uranio-schema": "ssh://git@github.com/nbl7/uranio-schema.git#master",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#master"
	}
}
